{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#fabd2f",
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "#d5c4a1",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "#83a598",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " \ue5ff {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#fabd2f",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fe8019{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fb4934{{ end }}",
            "{{ if gt .Ahead 0 }}#d3869b{{ end }}",
            "{{ if gt .Behind 0 }}#d3869b{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#b8bb26",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#fb4934{{ end }}"
          ],
          "template": " x{{ reason .Code }}"
        },
        {
          "foreground": "#b8bb26",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#fb4934{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#fb4934",
          "style": "plain",
          "template": "!",
          "type": "root"
        },
        {
          "foreground": "#fb4934",
          "style": "plain",
          "template": "‚ùØ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
